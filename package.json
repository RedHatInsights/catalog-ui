  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "extract:messages": "npx @formatjs/cli extract 'src/**/*.{js,jsx}' --out-file ./translations/messages.json",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "lint:js": "yarn eslint ./src --ext .js",
    "lint:ts": "yarn eslint ./src --ext .ts,.tsx",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:container": "NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development webpack serve --config config/dev.webpack.config.js --host 0.0.0.0",
    "test": "jest --verbose",
    "travis:build": "webpack --config config/test.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
