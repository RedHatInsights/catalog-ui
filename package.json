    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.3.0",
    "git-revision-webpack-plugin": "^3.0.4",
    "glob": "^7.1.6",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
